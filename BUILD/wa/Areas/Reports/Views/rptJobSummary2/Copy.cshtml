@{
    Layout = null;
    var nvc = HttpContext.Current.Request.QueryString;

    string strWhere = nvc["strWhere"] ?? "rptDate >= CAST('2016-07-01' AS DATE) AND rptDate <= CAST('2016-09-28' AS DATE) AND StatusKey = 100";
    string labelCriteria = nvc["labelCriteria"] ?? "";
    string strEmployeeKey = nvc["employeeKey"] ?? "";
    int employeeKey = 0;
    int.TryParse(strEmployeeKey, out employeeKey);
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <script type="text/javascript" src="~/Scripts/jquery-2.1.1.min.js"></script>
    <title>Report</title>
</head>
<body>
    <iframe id="iframe-wrapper" src="~/Areas/Reports/Report.aspx?employeeKey=@strEmployeeKey&labelCriteria=@labelCriteria&strWhere=@Uri.EscapeUriString(strWhere)" width="100%" height="100%"></iframe>
    <script>
        $(document).ready(function () {
            $("iframe").css({ 'height': ($(document).height() - ($(document).height()*0.05).toFixed(0)) + 'px' });
        });
    </script>
</body>
</html>
